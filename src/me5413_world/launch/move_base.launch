<launch>
  <!-- navigation common params-->
  <rosparam file="$(find me5413_world)/params/map_nav_params/local_costmap_params.yaml"      command="load" ns="move_base"/>
  <rosparam file="$(find me5413_world)/params/map_nav_params/global_costmap_params.yaml"     command="load" ns="move_base"/>
  <rosparam file="$(find me5413_world)/params/move_base_params.yaml"          command="load" ns="move_base"/>
  <rosparam file="$(find me5413_world)/params/base_global_planner_param.yaml" command="load" ns="move_base"/>
  <rosparam file="$(find me5413_world)/params/teb_local_planner_params.yaml"  command="load" ns="move_base"/>
  
  <param name="move_base/base_local_planner"    type="string" value="teb_local_planner/TebLocalPlannerROS"/>
  <!--<param name="base_global_planner" value="navfn/NavfnROS" />-->
  <param name="move_base/base_global_planner" type="string" value="global_planner/GlobalPlanner"/>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find me5413_world)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find me5413_world)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
  </node>
</launch>

